		usecase package org.eclipselabs.spray.requirements.shapes
		usecase package org.eclipselabs.spray.requirements.styling
		user interface application SprayDiagramEditor
		user interface flow Spray
		entity package org.eclipselabs.spray.requirements.entities.spray
		entity package org.eclipselabs.spray.requirements.entities.shapes
		entity package org.eclipselabs.spray.requirements.entities.styles
		entity package org.eclipselabs.spray.requirements.entities.mod4j
		usecase package org.eclipselabs.spray.requirements.examples.mod4j
		user interface application mod4j
		usecase package org.eclipselabs.spray.requirements.examples.bpmn20
		entity package org.eclipselabs.spray.requirements.entities.bpmn20
		user interface application bpmn20
		usecase package org.eclipselabs.spray.requirements.examples.lwc2012
		entity package org.eclipselabs.spray.requirements.entities.lwc2012
		user interface application lwc2012
		usecase package org.eclipselabs.spray.requirements.examples.petrinet
		entity package org.eclipselabs.spray.requirements.entities.petrinet
		user interface application petrinet
		usecase package org.eclipselabs.spray.requirements.examples.statemachine
		entity package org.eclipselabs.spray.requirements.entities.statemachine
		user interface application statemachine
		usecase package org.eclipselabs.spray.requirements.examples.nassishneiderman
