
module org.xspray.example.one.XsprayGenerator

import org.eclipse.emf.mwe.utils.*

Workflow {
	bean = StandaloneSetup {
		platformUri = "../.."
		registerGenModelFile = "platform:/resource/org.mod4j.dsl.businessdomain.mm/model/BusinessDomainDsl.genmodel"
	}
	component = @org.eclipselabs.spray.xtext.generator.XsprayGenerator auto-inject {
