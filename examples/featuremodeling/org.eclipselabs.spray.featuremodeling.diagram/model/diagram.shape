shape FeatureShape {
	rectangle {
		position (x=0, y=0)
		size (width=100, height=100)
		text {
			position (x=5, y=5)
			size (width=90, height=90)
			id = featureName
		}
	}
}

connection ArrowConnection {
	placing {
		position (offset=1.0)
		polygon {
			point (x=-10, y=-5)
			point (x=0, y=0)
			point (x=-10, y=5)
			style (background-color=black)
		}
	}
}
