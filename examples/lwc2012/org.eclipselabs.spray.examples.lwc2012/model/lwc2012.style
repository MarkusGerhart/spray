style LWC2012CorporateStyle {
	background-color = GreenToWhite
	line-color = dark-green
	gradient-orientation = horizontal
	highlighting(
		selected=GreenToOrange
		multiselected=GreenToYellow
		allowed=green
		unallowed=GreenToRed			
	)
}

