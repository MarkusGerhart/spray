module org.eclipselabs.spray.examples.petrinet.SprayGenerator

import org.eclipse.emf.mwe.utils.*

Workflow {
	bean = StandaloneSetup {
		platformUri = "../../.."
		registerGeneratedEPackage = "PetriNet.PetriNetPackage"
		registerGenModelFile = "platform:/resource/de.htwg.petrinet.mm/model/PetriNet.genmodel"
		scanClassPath=true
	}
	
	component = @org.eclipselabs.spray.generator.graphiti.SprayGenerator auto-inject {
		clean = "false"
		modelPath = "model,../de.htwg.petrinet.mm/model"
	}
}
