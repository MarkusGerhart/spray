<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.newWizards">
      <category
            id="spray_examples"
            name="Spray examples">
      </category>
      <category
            id="spray_official_examples"
            name="Official"
            parentCategory="spray_examples">
      </category>
      <wizard
            category="spray_examples/spray_official_examples"
            class="org.eclipse.emf.common.ui.wizard.ExampleInstallerWizard"
            id="org.eclipselabs.spray.examples.busmod"
            name="Mod4j business model"
            icon="icons/Spray-Logo-Icon-16.png"
            project="true">
         <description>
            Modeling of class diagrams (http://www.mod4j.org/documentation/1.3.0/user-guide/mod4j-user-guide.html#d0e685).
         </description>
      </wizard>
      <wizard
            category="spray_examples/spray_official_examples"
            class="org.eclipse.emf.common.ui.wizard.ExampleInstallerWizard"
            id="org.eclipselabs.spray.examples.bpmn"
            name="BPMN"
            icon="icons/Spray-Logo-Icon-16.png"
            project="true">
         <description>
            Workflow modeling with the Business Process Modeling Notation (BPMN - http://en.wikipedia.org/wiki/BPMN). 
         </description>
      </wizard>
      <wizard
            category="spray_examples/spray_official_examples"
            class="org.eclipse.emf.common.ui.wizard.ExampleInstallerWizard"
            id="org.eclipselabs.spray.examples.lwc2012"
            name="Heat exchanger"
            icon="icons/Spray-Logo-Icon-16.png"
            project="true">
         <description>
            Modeling of technical construction plans like that for a heat exchanger (http://www.languageworkbenches.net/images/1/19/LWC2012_Piping_%26_Instrumentation.pdf). 
         </description>
      </wizard>
      <wizard
            category="spray_examples/spray_official_examples"
            class="org.eclipse.emf.common.ui.wizard.ExampleInstallerWizard"
            id="org.eclipselabs.spray.examples.petrinet"
            name="Petrinet"
            icon="icons/Spray-Logo-Icon-16.png"
            project="true">
         <description>
            Modeling of execution behavior of distributed systems (http://en.wikipedia.org/wiki/Petrinets). 
         </description>
      </wizard>
