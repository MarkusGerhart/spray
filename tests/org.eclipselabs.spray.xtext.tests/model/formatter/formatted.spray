// Add import statements here
import BusinessDomainDsl.*

diagram mydiagram for BusinessDomainModel style org.eclipselabs.spray.runtime.graphiti.styles.DefaultSprayStyle

class BusinessClass {
	shape org.eclipselabs.spray.shapes.ClassShape {
		name into classname
		reference properties into attributes
		reference businessRules into businessrules
	}
	behavior {
		create into types palette "Knoten" askFor name
	}
}

class Association icon "connection16.gif" {
	connection ( ) {
		from source;
		to target;
		fromText text ( ) {
			"" + source.name
		}
	}
	behavior {
		create into associations palette "PaletteName" askFor name
	}
}