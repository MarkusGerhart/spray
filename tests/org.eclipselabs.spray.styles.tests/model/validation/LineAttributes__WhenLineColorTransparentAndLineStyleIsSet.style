/* XPECT_SETUP org.eclipselabs.spray.styles.tests.StyleValidationTest END_SETUP */

style MyStyle {
/* XPECT errors at 'transparent' ---
	"Setting the line-color to transparent isn't supported, use line-width = 0 instead."
--- */
	line-color = transparent
	line-width = 0
/* XPECT warnings at 'solid' ---
	"If the line-color is transparent, the line-style is deprecated." at "solid"
--- */
	line-style = solid
}